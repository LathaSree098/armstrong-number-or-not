#include<stdio.h>
void main()
{
    int n,temp,r,sum=0;
    printf("Enter the number:");
    scanf("%d",&n);
    temp=n;
    while(n!=0)
    {
       r=n%10;
       sum=sum+r*r*r;
       n=n/10;
    }
    if(sum==temp)
    {
       printf("given number is armstrong");
    }
    else
    {
       printf("%d number is not armstrong");
    }
}
